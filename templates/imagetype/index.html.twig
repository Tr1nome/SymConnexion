{% extends 'base.html.twig' %}

{% block title %}Imagetype index{% endblock %}

{% block body %}
    <h1>Imagetype index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for imagetype in imagetypes %}
            <tr>
                <td>{{ imagetype.id }}</td>
                <td>{{ imagetype.name }}</td>
                <td>
                    <a href="{{ path('imagetype_show', {'id': imagetype.id}) }}">show</a>
                    <a href="{{ path('imagetype_edit', {'id': imagetype.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('imagetype_new') }}">Create new</a>
{% endblock %}
